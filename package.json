{
  "name": "edb_docs",
  "private": true,
  "description": "EDB Docs",
  "version": "0.1.0",
  "scripts": {
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "config-sources": "python3 scripts/source/config_sources.py",
    "pull-sources": "python3 scripts/source/pull_sources.py",
    "format": "prettier --write src/**/*.{js,jsx}",
    "build": "gatsby build",
    "start": "gatsby serve",
    "update-icons": "git submodule update --init --remote && node scripts/createIconTypes.js && node scripts/createIconNames.js",
    "build-pdf": "python3 scripts/pdf/generate_pdf.py",
    "build-all-pdfs": "for i in docs/**/*/ ; do echo \"$i\"; python3 scripts/pdf/generate_pdf.py ${i%}; done",
    "fix-mtimes": "python3 scripts/source/restore_mtimes.py"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.1",
    "@mdx-js/react": "^1.6.1",
    "algoliasearch": "^4.2.0",
    "balance-text": "^3.3.0",
    "bootstrap": "4.5",
    "gatsby": "^2.22.20",
    "gatsby-background-image": "^0.9.19",
    "gatsby-cli": "^2.12.93",
    "gatsby-image": "^2.4.1",
    "gatsby-plugin-algolia": "^0.11.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.4.10",
    "gatsby-plugin-mdx": "^1.2.6",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-netlify": "^2.3.13",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-remove-fingerprints": "^0.0.2",
    "gatsby-plugin-sass": "^2.3.2",
    "gatsby-plugin-sharp": "^2.6.11",
    "gatsby-plugin-sitemap": "^2.4.13",
    "gatsby-remark-autolink-headers": "^2.3.1",
    "gatsby-remark-images": "^3.3.10",
    "gatsby-remark-prismjs": "^3.5.4",
    "gatsby-remark-static-images": "^1.2.0",
    "gatsby-source-filesystem": "^2.3.1",
    "gatsby-transformer-json": "^2.4.3",
    "gatsby-transformer-remark": "^2.8.15",
    "gatsby-transformer-sharp": "^2.5.1",
    "graceful-fs": "^4.2.4",
    "markdown-to-jsx": "^7.0.0",
    "node-sass": "^4.14.1",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-instantsearch-dom": "^6.5.0",
    "truncate-utf8-bytes": "^1.0.2"
  },
  "devDependencies": {
    "gh-pages": "^2.2.0",
    "prettier": "^1.19.1",
    "react-refresh": "^0.8.3",
    "typescript": "^3.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rocketinsights/edb_docs"
  }
}
