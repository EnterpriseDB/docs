{
  "name": "@enterprisedb/icons",
  "version": "0.0.0-semantically-released",
  "main": "./dist/index.mjs",
  "module": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./color": {
      "import": "./dist/color.mjs",
      "require": "./dist/color.cjs"
    },
    "./logos": {
      "import": "./dist/logos.mjs",
      "require": "./dist/logos.cjs"
    },
    "./flags": {
      "import": "./dist/flags.mjs",
      "require": "./dist/flags.cjs"
    }
  },
  "repository": {
    "url": "https://github.com/enterprisedb/fe"
  },
  "scripts": {
    "build-icons": "svgr -d compiled/ ../icons/src",
    "build-package": "rimraf dist && rollup -c",
    "build": "run-s build-icons format build-package",
    "format": "npx prettier --write .",
    "lint": "eslint --fix .",
    "semantic-release": "semantic-release --tag-format 'icons-placeholder${version}'",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@rollup/plugin-babel": "^5.3.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@svgr/cli": "^5.5.0",
    "@svgr/plugin-jsx": "^5.5.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest-dom": "^3.8.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-testing-library": "^4.2.0",
    "install": "^0.13.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "react": "*",
    "react-dom": "*",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.45.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-react-svg": "^3.0.3",
    "semantic-release": "^17.4.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
